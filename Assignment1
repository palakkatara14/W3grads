1.//Reverse integer
class Solution {
    public int reverse(int x) {
        long sum=0;
        int r;
        while(x!=0){
           r=x%10;
           sum=sum*10+r;
           x=x/10;
           
       
        
            if(sum>Integer.MAX_VALUE ||sum <Integer.MIN_VALUE){
                return 0;
            }
        }
        return (int)sum;

        
    }
}
2.//Palindrome Number
class Solution {
    public boolean isPalindrome(int x){
          int r;
          int temp;
          int sum=0;
          temp=x;
          
          while(x>0){
              r=x%10;
              x=x/10;
              sum=(sum*10)+r;
           }
           
           if(sum==temp){
               return true;
           }
           else{
               return false;
           }
    }
}
