import java.util.*;
public class Main{
     public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        System.out.println(sum_of_digits(n));
    }
    public static int sum_of_digits(int n){
       
        if(n==0){
            return 0;
        }
        return n%10+sum_of_digits(n/10);
    
    }
    
}
